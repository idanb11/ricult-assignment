{
  "name": "ms-store-quiz",
  "version": "0.0.1",
  "description": "Micro service for quizzes Web App",
  "author": "Idan Bercovici <idanb11@gmail.com>",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": ">=12.x.x",
    "npm": ">=6.11.3",
    "yarn": ">=1.17.3"
  },
  "scripts": {
    "start": "node index.js",
    "start_local": "nodemon -r dotenv/config --watch *.js --watch db-scri/*.js --watch ./config*.js index.js",
    "run_script:insert-quiz-data": "node -r dotenv/config db-scripts/insert-quiz-data.js --data=db-scripts/static-data/updated-quiz.json",
    "start:debug": "cross-env DEBUG=ms-store-quiz:* yarn start",
    "lint": "esw *.js server config --color",
    "lint:watch": "yarn lint -- --watch",
    "precommit": "yarn lint && yarn test",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --ui bdd --reporter spec --colors server --recursive",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover _mocha -- --ui bdd --reporter spec --colors server --recursive",
    "test:check-coverage": "yarn test:coverage && istanbul check-coverage",
    "report-coverage": "coveralls < ./coverage/lcov.info"
  },
  "repository": {
    "type": "bitbucket",
    "url": "git@bitbucket.org:ilmakiage/ms-store-quiz.git"
  },
  "keywords": [],
  "dependencies": {
    "@hapi/joi": "~16.1.8",
    "aws-sdk": "^2.634.0",
    "bluebird": "^3.7.2",
    "body-parser": "~1.19.0",
    "compression": "~1.7.4",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~4.1.1",
    "express": "~4.17.1",
    "express-jwt": "~5.3.1",
    "express-mongo-sanitize": "^1.3.2",
    "express-validation": "^1.0.3",
    "express-winston": "^4.0.3",
    "helmet": "~3.21.3",
    "http-status": "~1.4.2",
    "joi": "^14.3.1",
    "jsonwebtoken": "~8.5.1",
    "method-override": "~3.0.0",
    "minimist": "^1.2.0",
    "mongoose": "~5.9.3",
    "morgan": "~1.9.1",
    "object-hash": "^2.0.3",
    "redis": "^2.8.0",
    "winston": "~3.2.1"
  },
  "devDependencies": {
    "@types/express": "~4.17.3",
    "@types/hapi__joi": "~16.0.12",
    "@types/mongoose": "~5.7.3",
    "chai": "~4.2.0",
    "commitizen": "~4.0.3",
    "coveralls": "~3.0.9",
    "cross-env": "~6.0.3",
    "cz-conventional-changelog": "~3.1.0",
    "dotenv": "~8.2.0",
    "eslint": "~6.8.0",
    "eslint-config-airbnb-base": "~14.0.0",
    "eslint-plugin-import": "~2.20.1",
    "eslint-watch": "~6.0.1",
    "istanbul": "~1.1.0-alpha.1",
    "mocha": "^6.2.2",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2",
    "validate-commit-msg": "~2.14.0"
  },
  "license": "private",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
